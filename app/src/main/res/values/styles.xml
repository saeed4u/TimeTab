<resources>

    <style name="AutoCompleteTextViewTimeTab2" parent="android:Widget.Holo.Light.AutoCompleteTextView">
        <item name="android:dropDownSelector">@drawable/timetab2_list_selector_holo_light</item>
        <item name="android:background">@drawable/timetab2_edit_text_holo_light</item>
    </style>

    <style name="ButtonTimeTab2" parent="android:Widget.Holo.Light.Button">
        <item name="android:background">@drawable/timetab2_btn_default_holo_light</item>
    </style>

    <style name="ImageButtonTimeTab2" parent="android:Widget.Holo.Light.ImageButton">
        <item name="android:background">@drawable/timetab2_btn_default_holo_light</item>
    </style>

    <style name="OverFlow" parent="@android:style/Widget.Holo.ActionButton.Overflow">
        <item name="android:src">@drawable/ic_action_overflow</item>
    </style>

    <style name="SpinnerTimeTab2" parent="android:Widget.Holo.Light.Spinner">
        <item name="android:background">@drawable/timetab2_spinner_background_holo_light</item>
        <item name="android:dropDownSelector">@drawable/timetab2_list_selector_holo_light</item>
    </style>

    <style name="TabTimeTab2">
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingLeft">16dip</item>
        <item name="android:paddingRight">16dip</item>
        <item name="android:background">@drawable/timetab2_tab_indicator_holo</item>
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_weight">1</item>
        <item name="android:minWidth">80dip</item>
    </style>

    <style name="TabTextTimeTab2">
        <item name="android:textColor">#000000</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
        <!-- v14 <item name="android:textAllCaps">true</item> -->
        <item name="android:ellipsize">marquee</item>
        <item name="android:maxLines">2</item>
        <item name="android:maxWidth">180dip</item>
    </style>

    <style name="ProgressBarTimeTab2" parent="android:Widget.Holo.Light.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/timetab2_progress_horizontal_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/timetab2_progress_indeterminate_horizontal_holo_light</item>
    </style>

    <style name="SeekBarTimeTab2" parent="android:Widget.Holo.Light.SeekBar">
        <item name="android:progressDrawable">@drawable/timetab2_scrubber_progress_horizontal_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/timetab2_scrubber_progress_horizontal_holo_light</item>
        <item name="android:thumb">@drawable/timetab2_scrubber_control_selector_holo_light</item>
    </style>

    <style name="RatingBarTimeTab2" parent="android:Widget.Holo.Light.RatingBar">
        <item name="android:progressDrawable">@drawable/timetab2_ratingbar_full_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/timetab2_ratingbar_full_holo_light</item>
    </style>

    <style name="RatingBarBigTimeTab2" parent="android:Widget.Holo.Light.RatingBar.Indicator">
        <item name="android:progressDrawable">@drawable/timetab2_ratingbar_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/timetab2_ratingbar_holo_light</item>
    </style>

    <style name="RatingBarSmallTimeTab2" parent="android:Widget.Holo.Light.RatingBar.Small">
        <item name="android:progressDrawable">@drawable/timetab2_ratingbar_small_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/timetab2_ratingbar_small_holo_light</item>
    </style>

    <style name="ToggleTimeTab2" parent="android:Widget.Holo.Light.Button.Toggle">
        <item name="android:background">@drawable/timetab2_btn_toggle_holo_light</item>
    </style>

    <style name="TimeTab2" parent="@style/_TimeTab2" />

    <style name="_TimeTab2" parent="android:Theme.Holo.Light">
        <item name="android:editTextBackground">@drawable/timetab2_edit_text_holo_light</item>
        <item name="android:textColorHighlight">#99ff8a00</item>
        <item name="android:textSelectHandleLeft">@drawable/timetab2_text_select_handle_left</item>
        <item name="android:textSelectHandleRight">@drawable/timetab2_text_select_handle_right</item>
        <item name="android:textSelectHandle">@drawable/timetab2_text_select_handle_middle</item>
        <item name="android:autoCompleteTextViewStyle">@style/AutoCompleteTextViewTimeTab2</item>
        <item name="android:listChoiceIndicatorMultiple">@drawable/timetab2_btn_check_holo_light</item>
        <item name="android:listChoiceIndicatorSingle">@drawable/timetab2_btn_radio_holo_light</item>
        <item name="android:buttonStyle">@style/ButtonTimeTab2</item>
        <item name="android:imageButtonStyle">@style/ImageButtonTimeTab2</item>
        <item name="android:dropDownSpinnerStyle">@style/SpinnerTimeTab2</item>
        <item name="android:progressBarStyleHorizontal">@style/ProgressBarTimeTab2</item>
        <item name="android:seekBarStyle">@style/SeekBarTimeTab2</item>
        <item name="android:ratingBarStyle">@style/RatingBarTimeTab2</item>
        <item name="android:ratingBarStyleIndicator">@style/RatingBarBigTimeTab2</item>
        <item name="android:ratingBarStyleSmall">@style/RatingBarSmallTimeTab2</item>
        <item name="android:buttonStyleToggle">@style/ToggleTimeTab2</item>
        <item name="android:listChoiceBackgroundIndicator">@drawable/timetab2_list_selector_holo_light</item>
        <item name="android:activatedBackgroundIndicator">@drawable/timetab2_activated_background_holo_light</item>
        <item name="android:fastScrollThumbDrawable">@drawable/timetab2_fastscroll_thumb_holo</item>
        <item name="android:actionBarItemBackground">@drawable/selectable_background_timetab2</item>
        <item name="android:popupMenuStyle">@style/PopupMenu.Timetab2</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListView.Timetab2</item>
        <item name="android:actionBarTabStyle">@style/ActionBarTabStyle.Timetab2</item>
        <item name="android:actionDropDownStyle">@style/DropDownNav.Timetab2</item>
        <item name="android:actionBarStyle">@style/ActionBar.Solid.Timetab2</item>
        <item name="android:actionModeBackground">@drawable/cab_background_top_timetab2</item>
        <item name="android:actionModeSplitBackground">@drawable/cab_background_bottom_timetab2</item>
        <item name="android:actionModeCloseButtonStyle">@style/ActionButton.CloseMode.Timetab2</item>
        <item name="android:actionOverflowButtonStyle">@style/OverFlow</item>
    </style>

    <style name="ActionBar.Solid.Timetab2" parent="@android:style/Widget.Holo.Light.ActionBar.Solid">
        <item name="android:background">@drawable/ab_solid_timetab2</item>
        <item name="android:backgroundStacked">@drawable/ab_stacked_solid_timetab2</item>
        <item name="android:backgroundSplit">@drawable/ab_bottom_solid_timetab2</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Timetab2</item>
    </style>

    <style name="ActionBar.Transparent.Timetab2" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:background">@drawable/ab_transparent_timetab2</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Timetab2</item>
    </style>

    <style name="PopupMenu.Timetab2" parent="@android:style/Widget.Holo.Light.ListPopupWindow">
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_timetab2</item>
    </style>

    <style name="DropDownListView.Timetab2" parent="@android:style/Widget.Holo.Light.ListView.DropDown">
        <item name="android:listSelector">@drawable/selectable_background_timetab2</item>
    </style>

    <style name="ActionBarTabStyle.Timetab2" parent="@android:style/Widget.Holo.Light.ActionBar.TabView">
        <item name="android:background">@drawable/tab_indicator_ab_timetab2</item>
    </style>

    <style name="DropDownNav.Timetab2" parent="@android:style/Widget.Holo.Light.Spinner">
        <item name="android:background">@drawable/spinner_background_ab_timetab2</item>
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_timetab2</item>
        <item name="android:dropDownSelector">@drawable/selectable_background_timetab2</item>
    </style>

    <style name="ProgressBar.Timetab2" parent="@android:style/Widget.Holo.Light.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progress_horizontal_timetab2</item>
    </style>

    <style name="ActionButton.CloseMode.Timetab2" parent="@android:style/Widget.Holo.Light.ActionButton.CloseMode">
        <item name="android:background">@drawable/btn_cab_done_timetab2</item>
    </style>

    <!-- this style is only referenced in a Light.DarkActionBar based theme -->
    <style name="Theme.Timetab2.Widget" parent="@android:style/Theme.Holo">
        <item name="android:popupMenuStyle">@style/PopupMenu.Timetab2</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListView.Timetab2</item>
    </style>

</resources>